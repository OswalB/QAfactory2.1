<!doctype html>
<html lang="es">

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title id="title-main">appobq</title>
  <!-- Bootstrap CSS -->
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-9ndCyUaIbzAi2FUVXJi0CjmCapSmO7SnpJef0486qhLnuZ2cdeRhO02iuK6FUUVM" crossorigin="anonymous">
  <!--toastr - alerts-->
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/toastr.min.css">
  <!-- FONT AWESOME-->
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
  <!-- custom CSS-->
  <link rel="stylesheet" href="/css/main.css">
</head>



<body>
  <!-- NAVEGACION SUPERIOR -->
  <div class="nav-bar-top-container mx-0 p-0 d-flex ">
    <div id="nav-menu" class="text-white  d-inline-flex">
      <a class="btn align-self-center text-white btnmenu" id="btn-menu">
        <i class="fa fa-bars " aria-hidden="true"></i>
        <strong class="px-1">Mana</strong>
      </a>

    </div>
    <div id="nav-t" class=" ">
      {{>botones-navbartop}}
    </div>
  </div>

  <!-- SECCION QUE OCUPA EL ANCHO DE LA PANTALLA DEBAJO DE LA NAV SUPERIOR -->
  <div class="main-container d-flex">
    <!-- MENU LATERAL -->
    <nav class="sidebar" id="side-nav">
      <div class="header-box px-2 pt-3 pb-4">
        {{>botones-sidebar}}
      </div>
    </nav>
    <!-- CONTENIDO DE LA APLICACION -->
    <div class="container  px-lg-5 mx-lg-5">
      <div id="content-w" class="content-w"></div>
      <div id="alertFilter" class="alert alert-primary" role="alert" style="display:none">
        
      </div>
      <!-- barra de informcion de usuario -->
      <div class="text-secondary d-flex justify-content-between mx-1">
          <h5 class="text-dark">
            {{panel.titulo}}
          </h5>
          <span>
            {{#if user}}
                {{@root.nik}}
            {{else}}
                No ha iniciado sesi√≥n
            {{/if}}
          </span>
          <!-- Modal editor -->
<div class="modal fade" id="modalEditor" tabindex="-1" aria-labelledby="modalEditorLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="modal-titleEdit"></h5>
        
        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
      </div>
      <div class="modal-body">
        <form class="needs-validation was-validated" novalidate>
        <table class="table table-success table-striped">
          <thead>
            <tr>
              <th scope="col">Campo</th>
              <th scope="col">Valor</th>
            </tr>
          </thead>
          
          <tbody id="bodyTable">
            
          </tbody>
          
        </table>
      </form>
      </div>
      <div class="modal-footer">
        <a id="btn_reset" class="btn btn-secondary" href="#"><i class="fa fa-wrench fa-lg" aria-hidden="true"></i> Reset pass</a>
        <a _role="delete" id="btn_borrar" class="btn btn-danger" href="#"><i class="fa fa-times-circle fa-lg" aria-hidden="true"></i> Delete</a>
        <a _role="save" id="btn_guardar" class="btn btn-success" href="#"><i class="fa fa-floppy-o fa-lg" aria-hidden="true"></i> Guardar</a>
        <button type="button" class="btn btn-secondary" data-bs-dismiss="modal"><i class="fa fa-sign-out fa-lg" aria-hidden="true"></i> Salir</button>
      </div>
    </div>
  </div>
</div>
      </div>
      
        {{>messages}}
        {{>errors}}
        {{{body}}}
        {{#if panel.boton-pagination}}  
          {{>pagination}}
        {{/if}} 
        {{>footerpage}}
      
    </div>
  </div>

  {{#if panel.boton-xls}}
    {{>xls}}
  {{/if}}
  

  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-geWF76RCwLtnZ8qwWowPQNguL3RmwHVBC9FhGdlKrxdiJJigb/j/68SIy3Te4Bkz"
    crossorigin="anonymous"></script>

  <script src="https://code.jquery.com/jquery-3.6.0.min.js"
    integrity="sha256-/xUj+3OJU5yExlq6GSYGSHk7tPXikynS7ogEvDej/m4=" crossorigin="anonymous">
    </script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/toastr.min.js"></script>
  <script src="/js/main.js"></script>  
</body>

</html>