        </ul>
      </div>
      <div id="lotesFooter" class="modal-footer">
        <button _acc="delete" id="btnDeleteLote" type="button" class="btn btn-danger" disabled=""><i class="fa fa-ban fa-lg" aria-hidden="true"></i> Eliminar</button>
        <button _acc="exit" id="btnExitL" type="button" class="btn btn-secondary"  ><i class="fa fa-sign-out2 fa-lg" aria-hidden="true"></i> Cancelar</button>
        <button _acc="select" id="btnSelectLote" type="button" class="btn btn-primary"  disabled=""><i class="fa fa-sign-out fa-lg" aria-hidden="true"></i> Asignar</button>
      </div>
    </div>
