<!-- modal lotes list-->
<div  class="modal fade" id="lotesListModal" tabindex="-1" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="modalLabel">lotes disponibles:</h5>
      </div>
